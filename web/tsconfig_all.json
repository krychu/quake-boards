{
    "compilerOptions": {
        "noImplicitAny": true,
        "sourceMap": false,
        "alwaysStrict": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictNullChecks": true,
        "lib": ["es2016", "dom"],
        "module": "es6",
        "target": "es5",
        "outDir": "public",
        "rootDir": "ts"
    }
}
